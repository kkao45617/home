package test;

public class test {

	public static void main(String[] args) {
		abc a=new abc();
		a.setAbc(10);
		
		System.out.println(a.getAbc());
	}

}
class abc{
	
	private int a;
	private int b;
	
	public void setAbc(int a) {
		this.a=a;
	}
	public int getAbc() {
		return a;
	}
	
	public int sum() {
		return a+b;
	}
	
	

}
